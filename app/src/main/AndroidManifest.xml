<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vision.karunamgoyal.vision">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/visionapi"
        android:label="@string/app_name"
        android:roundIcon="@drawable/visionapi"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity
            android:name=".StudentActivity"
            android:label="@string/title_activity_student"
            android:theme="@style/AppTheme" />
        <activity android:name=".AppSettings" />
        <activity
            android:name=".ProfileSetting"
            android:label="@string/title_activity_profile_setting"
            android:theme="@style/AppTheme" />
        <activity android:name=".GetStarted" />
        <activity
            android:name=".DiscussActivity"
            android:label="@string/title_activity_discuss"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".CounsellorProfile"
            android:label="@string/title_activity_counsellor_profile"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".Profile"/>
        <activity android:name=".GetStarted2"/>
    </application>

</manifest>